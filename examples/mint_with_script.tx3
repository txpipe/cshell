party Customer;

policy VendingMachine = 0x3f2e288b4a19ab51d1775d2c6e397b43fad17cc8c47b988603136d73;

asset COIN = 0x3f2e288b4a19ab51d1775d2c6e397b43fad17cc8c47b988603136d73."COIN";

tx claim_tokens(
    pin: Bytes,
) {
    input gas {
        from: Customer,
        min_amount: fees,
    }

    collateral {
        from: Customer,
        min_amount: fees,
    }

    mint {
        amount: COIN(1),
        redeemer: pin,
    }

    output {
        to: Customer,
        amount: gas - fees + COIN(1),
    }

    cardano::plutus_witness {
        version: 3,
        script: ,
    }
}